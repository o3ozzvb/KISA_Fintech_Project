<%- layout('layout/layout') %>
        <div class="page-content">

            <div class="timeline-cover timeline-cover-center preload-image">
                <div data-height="230" class="caption shadow-large">
                    <div class="caption-center">
                        <img src="images/pictures/0t.jpg" alt="img"
                            class="preload-image horizontal-center round-image bottom-30 top-30" width="80">
                        <h1 class="center-text color-white bolder">Karla Black</h1>
                        <p class="boxed-text-huge under-heading color-white opacity-90">
                            Professional Photographer, Europe
                        </p>
                    </div>
                    <div class="caption-overlay bg-black opacity-80"></div>
                    <div class="caption-bg bg-27"></div>
                </div>
            </div>
            <div class="timeline-body">
                <div class="timeline-deco"></div>
                <% for(var i=0; i < tx_list.length ;i++){ %>
                <div class="timeline-item top-10">
                    <% if(tx_list[i].inout_type == '입금') {%>
                    <i class="fa fa-plus bg-pink2-dark shadow-large"></i>
                    <%}%>
                    <% if(tx_list[i].inout_type != '입금') {%>
                        <i class="fa fa-minus bg-blue2-dark shadow-large"></i>
                    <%}%>
                    <div class="timeline-item-content round-medium">
                        <span class="left-15 text-left"><%= tx_list[i].tran_date %><br></span>
                        <div class="left-15 text-left bolder" style="font-size:17px"><%= tx_list[i].print_content %></div>
                        <div class="right-15 text-right bolder" style="font-size:15px" ><%= tx_list[i].tran_amt %>원</div>
                    </div>
                </div>
                <% } %>
            </div>
        </div>
        <!-- Page Content Class Ends Here-->

        <div class="menu-hider"></div>
